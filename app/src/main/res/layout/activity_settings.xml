<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/dashboardFromSettings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DASHBOARD"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.947"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.971" />

    <EditText
        android:id="@+id/editTotalBudget"
        android:layout_width="132dp"
        android:layout_height="35dp"
        android:layout_marginTop="99dp"
        android:layout_marginEnd="38dp"
        android:layout_marginRight="38dp"
        android:layout_marginBottom="44dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/editBillsBudget"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editEntertainmentBudget"
        android:layout_width="132dp"
        android:layout_height="35dp"
        android:layout_marginEnd="38dp"
        android:layout_marginRight="38dp"
        android:layout_marginBottom="45dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/editOtherBudget"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/editOtherBudget"
        android:layout_width="132dp"
        android:layout_height="35dp"
        android:layout_marginEnd="38dp"
        android:layout_marginRight="38dp"
        android:layout_marginBottom="227dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/dashboardFromSettings"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/editFoodBudget"
        android:layout_width="132dp"
        android:layout_height="35dp"
        android:layout_marginEnd="38dp"
        android:layout_marginRight="38dp"
        android:layout_marginBottom="35dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/editEntertainmentBudget"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/editBillsBudget"
        android:layout_width="132dp"
        android:layout_height="35dp"
        android:layout_marginEnd="38dp"
        android:layout_marginRight="38dp"
        android:layout_marginBottom="39dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/editFoodBudget"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/BillsBudget"
        android:layout_width="180dp"
        android:layout_height="38dp"
        android:layout_marginStart="23dp"
        android:layout_marginLeft="23dp"
        android:layout_marginEnd="38dp"
        android:layout_marginRight="38dp"
        android:layout_marginBottom="36dp"
        android:text="Update Bills Budget"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/FoodBudget"
        app:layout_constraintEnd_toStartOf="@+id/editBillsBudget"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/FoodBudget"
        android:layout_width="180dp"
        android:layout_height="38dp"
        android:layout_marginStart="23dp"
        android:layout_marginLeft="23dp"
        android:layout_marginEnd="38dp"
        android:layout_marginRight="38dp"
        android:layout_marginBottom="32dp"
        android:text="Update Food Budget"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/EntertainmentBudget"
        app:layout_constraintEnd_toStartOf="@+id/editFoodBudget"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/EntertainmentBudget"
        android:layout_width="178dp"
        android:layout_height="45dp"
        android:layout_marginStart="23dp"
        android:layout_marginLeft="23dp"
        android:layout_marginEnd="38dp"
        android:layout_marginRight="38dp"
        android:layout_marginBottom="35dp"
        android:text="Update Entertainment Budget"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/OtherBudget"
        app:layout_constraintEnd_toStartOf="@+id/editEntertainmentBudget"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/OtherBudget"
        android:layout_width="178dp"
        android:layout_height="45dp"
        android:layout_marginStart="23dp"
        android:layout_marginLeft="23dp"
        android:layout_marginEnd="38dp"
        android:layout_marginRight="38dp"
        android:layout_marginBottom="284dp"
        android:text="Update Other Budget"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/editEntertainmentBudget"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/TotalBudget"
        android:layout_width="180dp"
        android:layout_height="38dp"
        android:layout_marginStart="23dp"
        android:layout_marginLeft="23dp"
        android:layout_marginTop="99dp"
        android:layout_marginEnd="38dp"
        android:layout_marginRight="38dp"
        android:layout_marginBottom="41dp"
        android:text="Update Total Budget"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/BillsBudget"
        app:layout_constraintEnd_toStartOf="@+id/editTotalBudget"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>